{
    "name": "symfony-cmf/file-editor-bundle",
    "description": "Bundle for editing files",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^2.8",
        "symfony-cmf/resource": "@dev",
        "symfony/twig-bundle": "^2.8"
    },
    "require-dev": {
        "symfony/console": "^2.8",
        "symfony/yaml": "^2.8",
        "symfony/process": "^2.8",
        "phpunit/phpunit": "@stable",
        "symfony-cmf/resource-bundle": "dev-description_enhancers"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\FileEditorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\FileEditorBundle\\Tests\\": "tests/",
            "Symfony\\Cmf\\Bundle\\FileEditorBundle\\Example\\": "example/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
